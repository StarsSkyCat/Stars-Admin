# 前端环境变量示例文件
# 请复制此文件为 .env.local 并根据实际情况修改配置

# 应用配置
VITE_APP_TITLE=Stars Admin 管理系统
VITE_APP_VERSION=1.0.0

# API 配置
VITE_API_BASE_URL=http://localhost:8080/api
VITE_API_TIMEOUT=10000

# 上传配置
VITE_UPLOAD_MAX_SIZE=10485760
VITE_UPLOAD_ALLOWED_TYPES=jpg,jpeg,png,gif,pdf,doc,docx

# 功能开关
VITE_ENABLE_MOCK=false
VITE_ENABLE_PWA=false
VITE_ENABLE_I18N=true

# 第三方服务
VITE_SENTRY_DSN=your-sentry-dsn-here
VITE_ANALYTICS_ID=your-analytics-id-here

# 开发配置
VITE_DEV_PROXY=true
VITE_DEV_HOST=localhost
VITE_DEV_PORT=3000

# 构建配置
VITE_BUILD_COMPRESS=true
VITE_BUILD_ANALYZE=false
VITE_BUILD_SOURCEMAP=false